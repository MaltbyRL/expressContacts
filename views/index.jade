extends layout

block content
  .container
    h1=title
    a.btn.btn-default(href='/addUser') Add Contact
    
    table.table
      thead
        tr
          th Name
          th Email
      tbody#userList
        each user in users
          tr
            td=user.name
            td=user.email
block scripts
  script(src='js/userIndex.js')       
  
